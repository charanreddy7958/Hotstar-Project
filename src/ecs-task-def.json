{
  "family": "react-app-task",
  "networkMode": "awsvpc",
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "256",
  "memory": "512",
  "executionRoleArn": "arn:aws:iam::552174866194:role/ecsTaskExecutionRole",
  "containerDefinitions": [
    {
      "name": "react-container",
      "image": "552174866194.dkr.ecr.us-west-1.amazonaws.com/my-react-app:latest",
      "essential": true,
      "portMappings": [
        {
          "containerPort": 80,
          "protocol": "tcp"
        }
      ]
    }
  ]
}
